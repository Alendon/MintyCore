<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:testMod="clr-namespace:TestMod"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TestMod.TestSubView"
             x:DataType="testMod:TestSubViewModel">
    
    <Design.DataContext>
        <testMod:TestSubViewModel />
    </Design.DataContext>
    
    <StackPanel>
        <TextBlock>Test sub View</TextBlock>
        <Button Command="{Binding TryCloseCommand}">Return</Button>

    </StackPanel>
</UserControl>
